# Production Configuration - BidEval
# Copy to .env.production and fill in values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-supabase-instance.com
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# n8n Webhook URLs (relative paths go through nginx proxy)
VITE_N8N_WEBHOOK_URL=/api/n8n/tabla
VITE_N8N_RFQ_INGESTA_URL=/api/n8n/ingesta-rfq
VITE_N8N_CHAT_URL=/api/n8n/chat-rfq
VITE_N8N_TABLA_URL=/api/n8n/tabla
VITE_N8N_MAIL_URL=/api/n8n/mail
VITE_N8N_QA_AUDIT_URL=/api/n8n/qa-audit-generator
VITE_N8N_SCORING_URL=/api/n8n/scoring-evaluation
VITE_N8N_QA_SEND_TO_SUPPLIER_URL=/api/n8n/qa-send-to-supplier
VITE_N8N_QA_PROCESS_RESPONSES_URL=/api/n8n/qa-process-responses
VITE_N8N_QA_SEND_EMAIL_URL=/api/n8n/qa-send-email
VITE_N8N_QA_PROCESS_EMAIL_RESPONSE_URL=/api/n8n/qa-process-email-response

# n8n REST API - DO NOT expose API key in frontend for production
# Move to a backend proxy that adds the key server-side
VITE_N8N_API_URL=/api/n8n-admin
# VITE_N8N_API_KEY should NOT be set here - use a server-side proxy

# Timeout (30 minutes)
VITE_REQUEST_TIMEOUT=1800000
